#: odoo.models.BaseModel._check_company odoo.models.BaseModel.create
#: odoo.models.BaseModel.ensure_one odoo.models.BaseModel.read
#: odoo.models.BaseModel.read_group odoo.models.BaseModel.search
#: odoo.models.BaseModel.unlink odoo.models.BaseModel.write
