msgid "(32000.00)"
msgstr "(32000.00)"

#: ../../content/applications/general/base_import/import_faq.rst:112
msgid "$ 32.000,00"
msgstr "$ 32.000,00"

#: ../../content/applications/general/base_import/import_faq.rst:113
msgid "(32000.00 €)"
msgstr "(32000.00 €)"

