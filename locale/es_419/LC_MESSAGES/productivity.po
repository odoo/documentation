msgid "Sample Value"
msgstr "Valor de ejemplo"

#: ../../content/applications/productivity/whatsapp.rst:679
msgid "Type"
msgstr "Tipo"

#: ../../content/applications/productivity/whatsapp.rst:680
msgid "Field"
msgstr "Campo"

